
doctype html
head
    meta(charset='utf-8')
    title Dashboard 7
    link(rel='stylesheet', type='text/css', href='/css/dc.css')
    // <link rel="stylesheet" type="text/css" href="/css/bootstrap.css" />
    style.
        body, html {
        height: 100%;
        margin: 0;
        padding: 0;
        }
        div {
        -webkit-box-sizing: border-box;
        -moz-box-sizing: border-box;
        box-sizing: border-box;
        }
        td {
        border: 1px solid gainsboro;
        }
        .info {
        background-color: pink;
        }
        div.row {
        display: block;
        height: 100%;
        width: 100%;
        }
        div.column {
        width: 33%;
        height: 100%;
        display: block;
        float: left;
        /*border:1px solid blue;*/
        }
        div.column div.subColumn {
        display: block;
        height: 50%;
        width: 50%;
        float: left;
        }
        div.column div.subRow {
        display: block;
        height: 50%;
        width: 100%;
        float: left;
        margin-top: 20px;
        }
        div.subRow2 {
        /*background-color: pink;*/
        display: block;
        height: 15%;
        margin-top: 8%;
        font-size: 3em;
        width: 100%;
        }
        div.dc-data-count {
        text-align: center;
        display: block;
        width: 100%;
        height: 5%;
        }
        h2 {
        margin-top: 20px;
        text-align: center;
        }
        table {
        border-collapse: collapse;
        margin: 0 auto;
        margin-top: 45px;
        }
        table thead tr,
        table tbody tr {
        border-bottom: 1px solid gainsboro;
        line-height: 2;
        }
        table tbody tr:hover {
        background-color: lightblue;
        cursor: pointer;
        }
        table thead tr {
        border-top: 1px solid gainsboro;
        background-color: steelblue;
        line-height: 2;
        color: white;
        }
        table tbody tr td,
        table thead tr td {
        width: 20%;
        text-align: center;
        }
.row
    .column
        #qty-chart.subColumn
            h2 Quantity
            span.reset(style='display: none;')
                span.filter
            a.reset(style='display:none', href='javascript:pieChart.filterAll(); dc.redrawAll();') reset
        #type-chart.subColumn
            h2 OrderQty
            span.reset(style='display: none;')
                span.filter
            a.reset(style='display:none', href='javascript:rowChart.filterAll(); dc.redrawAll();') reset
        #total-chart.subRow
            h2 Total over time
            span.reset(style='display: none;')
                span.filter
            a.reset(style='display:none', href='javascript:lineChart.filterAll(); dc.redrawAll();') reset
    #scatter-plot.column
        h2 Total-tip
        span.reset(style='display: none;')
            span.filter
        a.reset(style='display:none', href='javascript:scatterPlot.filterAll(); dc.redrawAll();') reset
        #number-display.subRow2 Total spend: 
    .column
        h2 Transactions
        .dc-data-count
            span.filter-count
            span.total-count
            a(href='javascript:dc.filterAll(); dc.renderAll();') Reset All
        table#payment-table
